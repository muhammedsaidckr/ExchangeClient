<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>exchange-client-1</PackageId>
    <Title>Exchange Client</Title>
    <Description>Exchange client gather all exchanges via jkorf and it uses common client sources</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Binance.Net" Version="8.4.4" />
    <PackageReference Include="Bitfinex.Net" Version="5.2.0" />
    <PackageReference Include="Bittrex.Net" Version="7.1.2" />
    <PackageReference Include="Bybit.Net" Version="2.0.0" />
    <PackageReference Include="CryptoExchange.Net" Version="5.4.3" />
    <PackageReference Include="Huobi.Net" Version="4.2.4" />
    <PackageReference Include="Kucoin.Net" Version="4.3.3" />
  </ItemGroup>

</Project>
